public class K19{
    public static void main(String[] args){
	char ch1, ch2;
	ch1 = 88;
	ch2 = 'Y';
	System.out.println(ch1 + " " + ch2);

	int num1,num2;
	num1 = 88;
	System.out.println(num1 + " " + ch2);

    }
}
//javac -d bin K19.java
//java -cp bin K19
//The character equivalent of 88 is X
//The integer equivalent of 88 is 88



